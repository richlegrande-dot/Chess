{
  "name": "walle-assistant-worker",
  "version": "1.0.0",
  "private": true,
  "description": "Standalone Cloudflare Worker for Wall-E chess coaching assistant",
  "type": "module",
  "scripts": {
    "build": "echo 'Worker uses TypeScript source directly - no build needed'",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "chess.js": "^1.0.0-beta.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "wrangler": "^3.22.1"
  }
}
