# Example environment variables for development
# Copy this file to .env and fill in your values

# Database URL for Prisma (uses Prisma Accelerate with Postgres)
DATABASE_URL="prisma+postgres://accelerate.prisma-data.net/?api_key=YOUR_API_KEY_HERE"

# Admin password for Knowledge Vault and system admin
ADMIN_PASSWORD="your_secure_password_here"

# OpenAI API key (optional - baseline coach works without it)
# OPENAI_API_KEY="sk-..."

# Rate limiting
# RATE_LIMIT_PER_IP=30
# RATE_LIMIT_WINDOW=60
