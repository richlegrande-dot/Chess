{
  "name": "worker-api",
  "version": "1.0.0",
  "description": "Single Cloudflare Worker API for ChessChat",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "env": "dotenv -e .dev.vars",
    "prisma:generate": "npm run env -- npx prisma generate",
    "prisma:migrate": "npm run env -- npx prisma migrate dev",
    "prisma:migrate:deploy": "npm run env -- npx prisma migrate deploy",
    "prisma:push": "npm run env -- npx prisma db push",
    "prisma:studio": "npm run env -- npx prisma studio",
    "test:e2e": "node test-learning-e2e.js",
    "test:learning": "node test-learning-e2e.js https://chesschat.uk",
    "verify:health": "node scripts/verify-learning-health.mjs --url https://chesschat.uk",
    "verify:ingest": "node scripts/ingest-sample-game.mjs --url https://chesschat.uk",
    "verify:concepts": "node scripts/verify-concept-states.mjs --url https://chesschat.uk",
    "verify:plan": "node scripts/verify-practice-plan.mjs --url https://chesschat.uk",
    "verify:intervention": "node scripts/verify-intervention-loop.mjs --url https://chesschat.uk",
    "verify:all": "bash verify-all.sh https://chesschat.uk",
    "verify:all:staging": "bash verify-all.sh https://chesschat.uk/api-staging"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@prisma/extension-accelerate": "^1.0.0",
    "chess.js": "^1.0.0-beta.6",
    "stockfish": "^17.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20.10.6",
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.8.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.1"
  }
}
